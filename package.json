{
  "name": "ts-rc-cli",
  "version": "0.0.5",
  "description": "React + Typescript Web 项目脚手架",
  "main": "dist/index.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "pu": "npm publish --access=public",
    "prepare": "npm run build:pkg",
    "build:pkg": "rm -rf ./dist && tsc -p ./tsconfig.json -d",
    "build:demo": "npm run build:pkg && node bin/ts-rc-build.js",
    "test": "npm run build:pkg && node bin/index.js",
    "get-config": "npm run build:pkg && cross-env CONFIG_PATH=create-test/config node dist/config/get-config.js "
  },
  "bin": {
    "ts-rc": "./bin/index.js",
    "ts-rc-create": "./bin/ts-rc-create.js",
    "ts-rc-dev": "./bin/ts-rc-dev.js",
    "ts-rc-build": "./bin/ts-rc-build.js"
  },
  "files": [
    "bin",
    "types",
    "react-ts-template",
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17",
    "typescript": ">=4.2.4",
    "webpack": ">=5.31.0"
  },
